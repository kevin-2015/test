<script>
	var tableFlag = 0;
	function hiddenTable(){
		if(tableFlag == 0){
			document.getElementById("specstable").style.display = "block";
			document.getElementById("eDFlod").className = "eDopen";
			tableFlag = 1;
		}else{
			document.getElementById("specstable").style.display = "none";
			document.getElementById("eDFlod").className = "eDFlod";
			tableFlag = 0;
		}
	}
	function comparePop(){
		console.log("####");
		var addInterChk=$(".addInter input").prop("checked"); 
		if(addInterChk){
			$(".compare").show();
		}else{
			$(".compare").hide();
		}
	}
</script>
