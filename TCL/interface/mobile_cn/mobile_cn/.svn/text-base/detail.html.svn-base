	function tab(flag){
	if(flag){
		$$("detail").style.display = "block";
		$$("gaisu").style.display = "none";
		$$("tab1").className += " act";
		$$("tab0").className = $$("tab0").className.replace(/ act/g,'');
	}else{
		$$("detail").style.display = "none";
		$$("gaisu").style.display = "block";
		$$("tab0").className += " act";
		$$("tab1").className = $$("tab1").className.replace(/ act/g,'');

	}
}

jQuery(function(){
	jQuery(".touch_page").on("swipeleft",function(){
   		$$("detail").style.display = "block";
		$$("gaisu").style.display = "none";
		$$("tab1").className += " act";
		$$("tab0").className = $$("tab0").className.replace(/ act/g,'');
  });
  jQuery("#detail").on("swiperight",function(){
    $$("detail").style.display = "none";
		$$("gaisu").style.display = "block";
		$$("tab0").className += " act";
		$$("tab1").className = $$("tab1").className.replace(/ act/g,'');
  });
  document.body.lastChild.remove();
})
