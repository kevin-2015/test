function close_wind(){
$$('mengban').className='ul_display_none';
$$('window_open').className='ul_display_none';
}
var tanchu_con_length= $$('tanchu_con').children.length;
function open_wind(id){
$$('mengban').className='mengban';
$$('window_open').className='window_open';
for(var i=0;i<tanchu_con_length;i++){
	if(id==i){
		$$('tanchu_con'+id).className='';
	}else{
		$$('tanchu_con'+i).className='ul_display_none';
	}
}
}
var menu_length = $$('menu_boader').children.length;
var menu_now=0;
function menu_rigt(id){
	if(id==1 || id==2){
	$$('menu_0').className='ul_float_left scqdss ul_margin_right ul_cursor_pointer now_ye';
	menu_now=0
	}else{
     if(menu_now== id){
	  	$$('menu_'+id).className='ul_float_left scqdss ul_margin_right ul_cursor_pointer now_ye';
	 }else{
	 	$$('menu_'+id).className='ul_float_left scqdss ul_margin_right ul_cursor_pointer now_ye'
	 	$$('menu_'+menu_now).className='ul_float_left scqdss ul_margin_right ul_cursor_pointer'
	 }
	 menu_now= id;
	 }
	for(var i = 0; i<menu_length;i++){
		if(id == i){
			$$('menu_boader'+id).className='';
		}else{
			$$('menu_boader'+i).className='ul_display_none';
		}
	}
}
function countries(){
var oFather = document.getElementById('countries_list');
var leng=oFather.children.length;
if(oFather.children.length>0){
							var id = oFather.firstElementChild.id;
							var index = id.substring(id.length-1,id.length);
							
							var oDiv = document.createElement("tr");
							oDiv.id = "countries"+leng+(parseInt(index)+1);
							var leng_id = leng+(parseInt(index)+1);
							oDiv.innerHTML ='<td width="15%">'+parseInt(leng+1)+'</td>'
											+'<td width="70%">&nbsp;</td>'
											+'<td width="15%" class="delect_add"><a class="ul_margin_right" href="#" onclick="countries_div('+leng+(parseInt(index)+1)+')">删除</a><a class="ul_margin_right" onclick="menu_rigt(1)">进入省市</a></td>';
										
							oFather.insertBefore(oDiv, oFather.firstElementChild);
						}else{						
							var oDiv = document.createElement("tr");
							oDiv.id = "countries"+(1);
							var leng_id= 1;
							oDiv.innerHTML = '<td width="15%">1</td>'
											+'<td width="70%">&nbsp;</td>'
											+'<td width="15%" class="delect_add"><a class="ul_margin_right" onclick="countries_div(1)">删除</a><a class="ul_margin_right" onclick="menu_rigt(1)">进入省市</a></td>';
										
							oFather.insertBefore(oDiv, oFather.firstElementChild);
						}
}

function countries_div(id){
	var guanli_div = document.getElementById("countries"+id);
	guanli_div.parentNode.removeChild(guanli_div);
}
function provinces(){
var oFather = document.getElementById('provinces_list');
var leng=oFather.children.length;
if(oFather.children.length>0){
							var id = oFather.firstElementChild.id;
							var index = id.substring(id.length-1,id.length);
							
							var oDiv = document.createElement("tr");
							oDiv.id = "provinces"+leng+(parseInt(index)+1);
							var leng_id = leng+(parseInt(index)+1);
							oDiv.innerHTML ='<td width="30%" class="tr_padding">'
												+'<div class="select_up">'
												+'<div class="ov_hiden">'
													+'<select class="select_nob" id="select'+leng_id+'">'
